    type: string
  code:
    type: integer
    required: true
  eventId:
    type: string
    required: true
  error:
    type: object
    required: true 
    properties:
      type:
        required: true
        type: string
        
      "title":
        required: true
        type: string
      
      details:
        required: true
        type: string